<?xml version="1.0" encoding="UTF-8"?>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	
	<diskStore path="c:\dev\mytempdir"/>
	<defaultCache maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="120"
		timeToLiveSeconds="600"
		overflowToDisk="true"
		diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="ai.liga.user.model.User"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="600"
		overflowToDisk="true" />

	<cache name="ai.liga.ligaai.model.Contact"
		maxElementsInMemory="5000"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="1800"
		overflowToDisk="true" />

	<cache name="ai.liga.ligaai.model.Tag"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="7200"
		overflowToDisk="true" />

	<cache name="ai.liga.ligaai.model.LigaAi"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
		
	<cache name="ai.liga.microurl.model.Microurl"
		maxElementsInMemory="100000"
		eternal="false"
		timeToIdleSeconds="600"
		timeToLiveSeconds="14400"
		overflowToDisk="true" />
		
    <cache name="SimplePageCachingFilter"
           maxElementsInMemory="10"
           eternal="false"
           timeToIdleSeconds="5"
           timeToLiveSeconds="20"
           overflowToDisk="true">
    </cache>
		
</ehcache>